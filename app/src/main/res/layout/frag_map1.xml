<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <ImageView
        android:id="@+id/TotalMap"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/map1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        >
    </ImageView>

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="72dp"
        android:fontFamily="@font/gangwondofont"
        android:textColor="#515151"
        android:text="경기도"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/text2"
        app:layout_constraintTop_toTopOf="@+id/text2" />

    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="228dp"
        android:layout_marginTop="76dp"
        android:fontFamily="@font/gangwondofont"
        android:text="강원도"
        android:textColor="#515151"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/TotalMap"
        app:layout_constraintTop_toTopOf="@id/TotalMap" />

    <TextView
        android:id="@+id/text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/gangwondofont"
        android:text="충청남도"
        android:textColor="#515151"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/text4"
        app:layout_constraintTop_toTopOf="@+id/text4" />

    <TextView
        android:id="@+id/text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="56dp"
        android:fontFamily="@font/gangwondofont"
        android:text="충청북도"
        android:textColor="#515151"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/text1"
        app:layout_constraintTop_toBottomOf="@+id/text1" />

    <TextView
        android:id="@+id/text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/gangwondofont"
        android:text="경상북도"
        android:textColor="#515151"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/text4"
        app:layout_constraintTop_toTopOf="@+id/text4"></TextView>

    <TextView
        android:id="@+id/text6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/gangwondofont"
        android:text="전라북도"
        android:textColor="#515151"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/text3"
        app:layout_constraintTop_toBottomOf="@+id/text3" />

    <TextView
        android:id="@+id/text7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="112dp"
        android:fontFamily="@font/gangwondofont"
        android:text="경상남도"
        android:textColor="#515151"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/text3"
        app:layout_constraintTop_toBottomOf="@+id/text3" />

    <TextView
        android:id="@+id/text8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/gangwondofont"
        android:text="전라남도"
        android:textColor="#515151"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/text6"
        app:layout_constraintTop_toBottomOf="@+id/text6" />

    <TextView
        android:id="@+id/text9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="36dp"
        android:fontFamily="@font/gangwondofont"
        android:text="제주도"
        android:textColor="#515151"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/text8"
        app:layout_constraintTop_toBottomOf="@+id/text8" />

</androidx.constraintlayout.widget.ConstraintLayout>